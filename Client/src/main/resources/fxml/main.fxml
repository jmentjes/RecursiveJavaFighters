<VBox prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
    <children>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" onAction="#logout" text="Logout" />
